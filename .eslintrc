{
  "extends": [
    "airbnb-standard",
    "plugin:jsx-a11y/recommended"
  ],
  parser: 'babel-eslint',
  parserOptions: {
    ecmaVersion: 7,
    sourceType: 'module',
    ecmaFeatures: {
      jsx: true,
      experimentalObjectRestSpread: true,
    }
  },
  "plugins": [
    "jsx-a11y",
    "react",
    # "mocha",
    "import"
  ],
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }],
    "no-array-index-key": "off"
  }
}
